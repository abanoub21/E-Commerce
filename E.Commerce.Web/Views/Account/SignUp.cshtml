@model E.Commerce.Web.BackendServices.UserRoleDto
@{
    Layout = null;
}


<script src="~/lib/jquery/dist/jquery.js"></script>
<link href="~/css/account/signup.css" rel="stylesheet" />

<body>
    
    <div class="container">

        <div class="login-box">

            <h2>Sign Up</h2>
            <form asp-controller="Account" asp-action="SignUp" method="post">
                <div class="input-box">
                    <input type="text" asp-for="UserName" required>
                    <label>Enter Your Name</label>
                </div>
                <div class="input-box">
                    <input type="email" asp-for="Email" required>
                    <label>Enter Your Email</label>
                </div>
                <div class="input-box">
                    <input type="text" asp-for="MobileNo" required>
                    <label>Enter Your Phone</label>
                </div>
                <div class="input-box">
                    <input type="password"  asp-for="Password" required>
                    <label> Password </label>
                </div>
                <div class="input-box">
                    <input type="password" asp-for="Password" required>
                    <label> Confirm Password </label>
                </div>
               
                <button type="submit" class="btn">Submit</button>
              
            </form>
        </div>
